<external.com.google.android.apps.iosched.ui.widget.DashboardLayout 
  	xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/home_dash1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <Button android:id="@+id/home_btn_student_area"
        style="@style/DashboardButton"
        android:text="@string/btn_student_area"
        android:drawableTop="@drawable/home_btn_sessions" />
        
    <Button android:id="@+id/home_btn_student_services"
        style="@style/DashboardButton"
        android:text="@string/btn_student_services"
        android:drawableTop="@drawable/home_btn_printer" />
    
    <Button android:id="@+id/home_btn_friends"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/home_btn_friends"
        android:text="@string/btn_friends"/>
    
    <Button android:id="@+id/home_btn_news"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/home_btn_news"
        android:text="@string/btn_news"/> 
        
     <Button android:id="@+id/home_btn_map"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/home_btn_map"
        android:text="@string/btn_map"/> 
        
     <Button android:id="@+id/home_btn_settings"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/home_btn_settings"
        android:text="@string/btn_settings"/>      

</external.com.google.android.apps.iosched.ui.widget.DashboardLayout>
